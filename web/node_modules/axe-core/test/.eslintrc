{
  "extends": ["prettier"],
  "env": {
    "browser": true
  },
  "globals": {
    "describe": true,
    "it": true,
    "xit": true,
    "before": true,
    "beforeEach": true,
    "after": true,
    "afterEach": true,
    "chai": true,
    "assert": true,
    "axe": true,
    "helpers": true
  },
  "rules": {
    "no-bitwise": 2,
    "camelcase": 2,
    "curly": 2,
    "eqeqeq": 2,
    "guard-for-in": 2,
    "wrap-iife": [
      2,
      "any"
    ],
    "no-use-before-define": 0,
    "new-cap": 2,
    "no-caller": 2,
    "no-empty": 2,
    "no-new": 2,
    "no-plusplus": 0,
    "no-undef": 2,
    "no-unused-vars": 2,
    "strict": 0,
    "max-params": [
      2,
      8
    ],
    "max-depth": [
      2,
      15
    ],
    "max-statements": [
      2,
      250
    ],
    "complexity": [
      2,
      50
    ],
    "no-cond-assign": 0,
    "no-debugger": 0,
    "no-eq-null": 0,
    "no-eval": 0,
    "no-unused-expressions": 0,
    "block-scoped-var": 0,
    "no-iterator": 0,
    "no-loop-func": 0,
    "no-multi-str": 0,
    "no-proto": 0,
    "no-script-url": 0,
    "dot-notation": 2,
    "no-new-func": 0,
    "no-new-wrappers": 0
  }
}
