{
  "name": "lmnts-gatsby-starter",
  "license": "MIT",
  "author": "Peter Laxalt <pete@laxaltandmciver.co>",
  "scripts": {
    "start": "npm run develop",
    "clean": "rimraf .cache public",
    "dev": "bash castle.sh && npm run clean && gatsby develop -S",
    "push": "npm run build && git add . && gitmoji -c && git push",
    "serve": "gatsby serve",
    "ssr": "npm run build && npm run serve",
    "format": "prettier --config ./.prettierrc --write src/**/*.js",
    "build:dir": "mkdirp ./public/icons/",
    "build:favicons": "node scripts/favicons",
    "build": "bash bam.sh && npm run clean && npm run build:dir && npm run build:favicons && gatsby build",
    "test": "ava **/*.test.js --verbose"
  },
  "greenkeeper": {
    "ignore": [
      "ava"
    ]
  },
  "dependencies": {
    "@reach/dialog": "^0.1.2",
